        private void SyncDictionaryBulk(List<Dictionary> dictionaryList, String user)
        {
            Int32 chunkSize = 2000;
            Int32 dictionaryCount = dictionaryList.Count;
            
            Dictionary<Int32, List<Dictionary>> dataChunks = [];

            
            Int32 chuckCount = 0;
            Int32 index = 0;
            
            foreach (Dictionary dict in dictionaryList)
            {
                if (chuckCount == chunkSize - 1)
                {
                    chuckCount = 0;
                    index++;
                }
                
                
                
                chuckCount ++;
            }
            
            DynamicParameters parameters = new DynamicParameters();

            parameters.Add("P__Dictionary", GetDictionaryDt(dictionaryList).AsTableValuedParameter());
            parameters.Add("P__User", user);

            _dapperAccess.Execute("usp_Sync_Dictionary_Bulk", parameters);
        }
