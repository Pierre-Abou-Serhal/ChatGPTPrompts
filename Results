AND (
    (CASE WHEN t.Sdx_GivenNames  = nc.Sdx_GivenNames  THEN 1 ELSE 0 END) +
    (CASE WHEN t.Sdx_FamilyName  = nc.Sdx_FamilyName  THEN 1 ELSE 0 END) +
    (CASE WHEN t.Sdx_FatherName  = nc.Sdx_FatherName  THEN 1 ELSE 0 END) +
    (CASE WHEN t.Sdx_MoFirstName = nc.Sdx_MoFirstName THEN 1 ELSE 0 END)
) >= 2
