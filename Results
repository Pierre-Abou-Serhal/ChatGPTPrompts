      ThreeDS.configure({
        merchantId,
        sessionId,
        containerId: '3DSUI',
        callback: function () {
          if (ThreeDS.isConfigured()) {
            log('ThreeDS configured');
            setStatus('SDK configured âœ”', 'ok');
            els.btnInitiate.disabled = false;
            els.btnAuthenticate.disabled = false;
          }
        },
        configuration: {
          userLanguage: navigator.language || 'en-US',
          wsVersion
        }
      });
