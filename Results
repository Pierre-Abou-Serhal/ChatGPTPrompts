public class DuplicateCustomerHistoryDto
{
    public Int32 NewCustomerId { get; set; }
    public Int32 CustomerId { get; set; }

    // New Customer Fields
    public String New_DateOfBirth { get; set; }
    public String New_GivenNames { get; set; }
    public String New_FamilyName { get; set; }
    public String New_FatherName { get; set; }
    public String New_MoFirstName { get; set; }
    public String New_BirthRegister { get; set; }
    public String New_BirthRegNo { get; set; }
    public String New_Gender { get; set; }

    // Original Customer Fields
    public String Db_DateOfBirth { get; set; }
    public String Db_GivenNames { get; set; }
    public String Db_FamilyName { get; set; }
    public String Db_FatherName { get; set; }
    public String Db_MoFirstName { get; set; }
    public String Db_BirthRegister { get; set; }
    public String Db_BirthRegNo { get; set; }
    public String Db_Gender { get; set; }

    // Total marks per field
    public Decimal Total_Mark_DateOfBirth { get; set; }
    public Decimal Total_Mark_GivenNames { get; set; }
    public Decimal Total_Mark_FamilyName { get; set; }
    public Decimal Total_Mark_FatherName { get; set; }
    public Decimal Total_Mark_MoFirstName { get; set; }
    public Decimal Total_Mark_BirthRegister { get; set; }
    public Decimal Total_Mark_BirthRegNo { get; set; }
    public Decimal Total_Mark_Gender { get; set; }
    public Decimal MinimumPassingMark { get; set; }

    // Computed marks per field
    public Decimal Mark_DateOfBirth { get; set; }
    public Decimal Mark_GivenNames { get; set; }
    public Decimal Mark_FamilyName { get; set; }
    public Decimal Mark_FatherName { get; set; }
    public Decimal Mark_MoFirstName { get; set; }
    public Decimal Mark_BirthRegister { get; set; }
    public Decimal Mark_BirthRegNo { get; set; }
    public Decimal Mark_Gender { get; set; }
}
