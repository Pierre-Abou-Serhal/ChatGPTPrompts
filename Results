UPDATE fic
SET fic.SourceAccountNumber = f.SourceAccountNumber
FROM t_File_Import_Content AS fic
JOIN t_File_Import        AS fi ON fi.Id   = fic.t_File_Import_Id   -- change col name
JOIN t_File               AS f  ON f.Id    = fi.t_File_Id           -- change col name
WHERE fic.SourceAccountNumber <> f.SourceAccountNumber
   OR fic.SourceAccountNumber IS NULL;
