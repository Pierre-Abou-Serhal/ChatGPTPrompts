$src = "http://uat-azure/DEV/_packaging/azure-devops-dev-nuget-offline-packages/nuget/v3/index.json"
$root = "$env:USERPROFILE\Downloads\NugetPackages"

Get-ChildItem $root -Recurse -Filter *.nupkg |
  Where-Object { $_.Name -notlike "*.symbols.nupkg" } |
  ForEach-Object {
    Write-Host "Pushing $($_.FullName)"
    nuget push -Source $src -ApiKey az -NonInteractive -SkipDuplicate $_.FullName
  }
