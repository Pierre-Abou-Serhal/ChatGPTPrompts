export function getDuplicatedAccNbrs(items: FileImportContent[]): string[] {
  const counts = new Map<string, number>();

  for (const item of items) {
    const acc = item.accNbr?.trim();
    if (!acc) continue; // ignore undefined/empty/whitespace
    counts.set(acc, (counts.get(acc) ?? 0) + 1);
  }

  const duplicates: string[] = [];
  for (const [acc, count] of counts) {
    if (count > 1) duplicates.push(acc);
  }

  return duplicates;
}
