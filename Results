public bool ValidateObjects(IEnumerable<MyObject> objects)
{
    if (objects == null || !objects.Any())
    {
        throw new ArgumentException("The collection of objects cannot be null or empty.");
    }
    
    // Ensure all arrays have the same length as the first array.
    bool sameLength = objects.Select(o => o.Data.Length)
                               .Distinct()
                               .Count() == 1;
    
    // Ensure no string in any array is null or empty.
    bool noEmptyStrings = objects.All(o => o.Data.All(s => !string.IsNullOrEmpty(s)));
    
    return sameLength && noEmptyStrings;
}
