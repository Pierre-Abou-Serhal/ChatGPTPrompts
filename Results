public Int32 LinkCustomer(DuplicateCustomerDto DuplicateCustomerDto, String User)
{
    DynamicParameters param = new DynamicParameters();

    param.Add("NewCustomerId", DuplicateCustomerDto.NewCustomerId, dbType: DbType.Int64, direction: ParameterDirection.Input);

    if (DuplicateCustomerDto.CustomerId == 0)
    {
        param.Add("CustomerId", null, dbType: DbType.Int64, direction: ParameterDirection.Input);
    }
    else
    {
        param.Add("CustomerId", DuplicateCustomerDto.CustomerId, dbType: DbType.Int64, direction: ParameterDirection.Input);
    }

    param.Add("New_DateOfBirth", DuplicateCustomerDto.New_DateOfBirth, dbType: DbType.String, direction: ParameterDirection.Input);
    param.Add("New_GivenNames", DuplicateCustomerDto.New_GivenNames, dbType: DbType.String, direction: ParameterDirection.Input);
    param.Add("New_FamilyName", DuplicateCustomerDto.New_FamilyName, dbType: DbType.String, direction: ParameterDirection.Input);
    param.Add("New_FatherName", DuplicateCustomerDto.New_FatherName, dbType: DbType.String, direction: ParameterDirection.Input);
    param.Add("New_MoFirstName", DuplicateCustomerDto.New_MoFirstName, dbType: DbType.String, direction: ParameterDirection.Input);
    param.Add("New_BirthRegister", DuplicateCustomerDto.New_BirthRegister, dbType: DbType.String, direction: ParameterDirection.Input);
    param.Add("New_BirthRegNo", DuplicateCustomerDto.New_BirthRegNo, dbType: DbType.String, direction: ParameterDirection.Input);
    param.Add("New_Gender", DuplicateCustomerDto.New_Gender, dbType: DbType.String, direction: ParameterDirection.Input);

    param.Add("Db_DateOfBirth", DuplicateCustomerDto.Db_DateOfBirth, dbType: DbType.String, direction: ParameterDirection.Input);
    param.Add("Db_GivenNames", DuplicateCustomerDto.Db_GivenNames, dbType: DbType.String, direction: ParameterDirection.Input);
    param.Add("Db_FamilyName", DuplicateCustomerDto.Db_FamilyName, dbType: DbType.String, direction: ParameterDirection.Input);
    param.Add("Db_FatherName", DuplicateCustomerDto.Db_FatherName, dbType: DbType.String, direction: ParameterDirection.Input);
    param.Add("Db_MoFirstName", DuplicateCustomerDto.Db_MoFirstName, dbType: DbType.String, direction: ParameterDirection.Input);
    param.Add("Db_BirthRegister", DuplicateCustomerDto.Db_BirthRegister, dbType: DbType.String, direction: ParameterDirection.Input);
    param.Add("Db_BirthRegNo", DuplicateCustomerDto.Db_BirthRegNo, dbType: DbType.String, direction: ParameterDirection.Input);
    param.Add("Db_Gender", DuplicateCustomerDto.Db_Gender, dbType: DbType.String, direction: ParameterDirection.Input);

    param.Add("Total_Mark_DateOfBirth", DuplicateCustomerDto.Total_Mark_DateOfBirth, dbType: DbType.String, direction: ParameterDirection.Input);
    param.Add("Total_Mark_GivenNames", DuplicateCustomerDto.Total_Mark_GivenNames, dbType: DbType.String, direction: ParameterDirection.Input);
    param.Add("Total_Mark_FamilyName", DuplicateCustomerDto.Total_Mark_FamilyName, dbType: DbType.String, direction: ParameterDirection.Input);
    param.Add("Total_Mark_FatherName", DuplicateCustomerDto.Total_Mark_FatherName, dbType: DbType.String, direction: ParameterDirection.Input);
    param.Add("Total_Mark_MoFirstName", DuplicateCustomerDto.Total_Mark_MoFirstName, dbType: DbType.String, direction: ParameterDirection.Input);
    param.Add("Total_Mark_BirthRegister", DuplicateCustomerDto.Total_Mark_BirthRegister, dbType: DbType.String, direction: ParameterDirection.Input);
    param.Add("Total_Mark_BirthRegNo", DuplicateCustomerDto.Total_Mark_BirthRegNo, dbType: DbType.String, direction: ParameterDirection.Input);
    param.Add("Total_Mark_Gender", DuplicateCustomerDto.Total_Mark_Gender, dbType: DbType.String, direction: ParameterDirection.Input);
    param.Add("MinimumPassingMark", DuplicateCustomerDto.MinimumPassingMark, dbType: DbType.String, direction: ParameterDirection.Input);

    param.Add("Mark_DateOfBirth", DuplicateCustomerDto.Mark_DateOfBirth, dbType: DbType.String, direction: ParameterDirection.Input);
    param.Add("Mark_GivenNames", DuplicateCustomerDto.Mark_GivenNames, dbType: DbType.String, direction: ParameterDirection.Input);
    param.Add("Mark_FamilyName", DuplicateCustomerDto.Mark_FamilyName, dbType: DbType.String, direction: ParameterDirection.Input);
    param.Add("Mark_FatherName", DuplicateCustomerDto.Mark_FatherName, dbType: DbType.String, direction: ParameterDirection.Input);
    param.Add("Mark_MoFirstName", DuplicateCustomerDto.Mark_MoFirstName, dbType: DbType.String, direction: ParameterDirection.Input);
    param.Add("Mark_BirthRegister", DuplicateCustomerDto.Mark_BirthRegister, dbType: DbType.String, direction: ParameterDirection.Input);
    param.Add("Mark_BirthRegNo", DuplicateCustomerDto.Mark_BirthRegNo, dbType: DbType.String, direction: ParameterDirection.Input);
    param.Add("Mark_Gender", DuplicateCustomerDto.Mark_Gender, dbType: DbType.String, direction: ParameterDirection.Input);

    param.Add("User", User, DbType.String, direction: ParameterDirection.Input);

    Int32 newCustomerId = DapperDBAccess.ExecuteSelectCommand<Int32>("usp_Alterna_LinkCustomer", param, CommandType.StoredProcedure).First();

    return newCustomerId;
}
